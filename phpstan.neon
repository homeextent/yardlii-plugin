# phpstan.neon
includes:
    - phpstan-baseline.neon

parameters:
    level: 6
    paths:
        - includes
        - tests/unit
    excludePaths:
        - vendor/*
        - assets/*
        - tests/integration  # <--- Changed from tests/integration/* to just tests/integration to be safer
    bootstrapFiles:
        - tests/phpstan-bootstrap.php
    reportUnmatchedIgnoredErrors: false